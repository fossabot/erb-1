    "node-sass": "^4.13.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-async-ssr": "^0.5.2",
    "react-cookie": "^4.0.1",
    "react-dev-utils": "^9.1.0",
